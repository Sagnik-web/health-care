{ 
    "version": 2, 
    "builds":[
        {
            "src":"server.js", 
            "use":"@vercel/node"
        },
         {
            "src":"build/**", 
            "use":"@vercel/static"
        }
    
    ],
    "routes":[
        {
            "src":"/api/user",
            "dest":"server.js"
        },
         {
            "src":"/api/user/(.*)",
            "dest":"server.js"
        },
         {
            "src":"/api/admin",
            "dest":"server.js"
        },
        {
            "src":"/api/admin/(.*)",
            "dest":"server.js"
        },
         {
            "src":"/api/doctor",
            "dest":"server.js"
        },
        {
            "src":"/api/doctor/(.*)",
            "dest":"server.js"
        },

        {
            "src":"/",
            "dest":"build/index.html"
        },
        {
            "src":"/(.+)",
            "dest":"build/$1"
        },
        
    ]
}
